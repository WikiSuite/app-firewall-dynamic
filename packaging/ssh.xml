<?xml version="1.0" encoding="UTF-8"?>
  <config name="ssh" basename="firewall_dynamic">
  <description>firewall_dynamic_ssh_description</description>
  <trigger>firewall_dynamic_ssh_trigger</trigger>
  <window>30</window>
  <enabled>1</enabled>
  <group>allusers</group>
  <table name="filter">
    <chain name="INPUT">
      <rule>
        <position>INSERT</position>
        <conditions>
          <match>
            <p>tcp</p>
          </match>
          <match>
            <s></s>
          </match>
          <match explicit="tcp">
            <dport></dport>
          </match>
          <match explicit="state">
            <state>NEW,ESTABLISHED</state>
          </match>
            <match explicit="time">
            <datestop></datestop>
          </match>
        </conditions>
        <jump>ACCEPT</jump>
      </rule>
    </chain>
  </table>
</config>
